{
  "name": "simple-js-sdk",
  "version": "0.0.1",
  "description": "A simple JavaScript SDK.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --source-maps && yarn run webpack",
    "webpack": "webpack",
    "test": "jest",
    "test:watch": "nodemon --watch src --watch test --exec 'yarn run test'",
    "doc": "jsdoc src -d docs",
    "doc:watch": "nodemon --watch src --exec 'yarn run doc'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blutarche/simple-js-sdk.git"
  },
  "keywords": [
    "sdk"
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.6.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.18.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "author": "Aikdanai Sidhikosol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blutarche/simple-js-sdk/issues"
  },
  "homepage": "https://github.com/blutarche/simple-js-sdk#readme",
  "dependencies": {
    "jest": "^23.6.0"
  }
}
